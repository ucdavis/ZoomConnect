@model ZoomConnect.Web.Services.Zoom.CachedMeetingModels

@{
    ViewData["Title"] = "Courses";
}

<h1>Courses</h1>

<p>ssrmeet row count: @ViewData["RowCount"]</p>

<h2>Meetings (@Model.Meetings.Count)</h2>
<ul>
    @foreach (var meeting in Model.Meetings
     .OrderBy(m => m.bannerCourse.crse_numb)
     .ThenBy(m => m.bannerCourse.seq_numb))
    {
        <li>
            @meeting.bannerCourse.subj_code @meeting.bannerCourse.crse_numb [@meeting.bannerCourse.seq_numb] -
            @meeting.bannerCourse.crse_title
            (
            <b>@meeting.primaryProf?.bannerPerson?.last_name</b>
            @foreach (var otherProf in meeting.otherProfs)
            {
                <span>,</span>
                @otherProf.bannerPerson.last_name
            }
            )
        </li>
    }
</ul>